gitignore
# Built application files
*.apk
*.aar
*.ap_
*.aab

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/

# Gradle files
.gradle/
build/

# Local configuration file (sdk.dir is unique to each computer)
local.properties

# Proguard folder generated by Eclipse
proguard/

# Log Files
*.log

# Android Studio Navigation editor temp files
*.navigation/

# Android Studio captures folder
captures/

# IntelliJ IDEA / Android Studio
.idea/
*.iml
*.ipr
*.iws
*.name
misc.xml
deploymentTargetDropDown.xml
render.experimental.xml

# Keystore files (Never upload these!)
*.jks
*.keystore
*.pem
*.p12
*.key

# External native build folder
.cxx/
.externalNativeBuild/

# Google Services (e.g. for Firebase)
google-services.json

# OS specific
.DS_Store
Thumbs.db
